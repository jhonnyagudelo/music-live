---
import type {IconProps} from '../../interfaces/iconTypes'
const { width = '1em', height = '1em', className } = Astro.props as IconProps;

---

<svg class={className}
    xmlns="http://www.w3.org/2000/svg" 
    width={width}
    height={height} 
    viewBox="0 0 24 24">
    <path 
        fill="none" 
        stroke="currentColor" 
        stroke-linecap="round" 
        stroke-linejoin="round" 
        stroke-miterlimit="10" 
        stroke-width="1.5" 
        d="m21 21l-4-4m2-6a8 8 0 1 1-16 0a8 8 0 0 1 16 0"
    />
</svg>

